{
    "baseURL": "https://rewards.bing.com",
    "sessionPath": "/root/sukaka/rewards/sessions",
    "headless": false,
    "parallel": false,
    "runOnZeroPoints": false,
    "clusters": 1,
    "saveFingerprint": {
        "mobile": true,
        "desktop": true
    },
    "workers": {
        "doDailySet": true,
        "doMorePromotions": true,
        "doPunchCards": true,
        "doDesktopSearch": true,
        "doMobileSearch": true,
        "doDailyCheckIn": true,
        "doReadToEarn": true
    },
    "searchOnBingLocalQueries": true,
    "globalTimeout": "300s",
    "searchSettings": {
        "useGeoLocaleQueries": false,
        "scrollRandomResults": true,
        "clickRandomResults": true,
        "searchDelay": {
            "min": "3min",
            "max": "6min"
        },
        "retryMobileSearchAmount": 2
    },
    "logExcludeFunc": [
        "SEARCH-CLOSE-TABS"
    ],
    "webhookLogExcludeFunc": [
        "SEARCH-CLOSE-TABS"
    ],
    "proxy": {
        "proxyGoogleTrends": true,
        "proxyBingTerms": true
    },
    "webhook": {
        "enabled": false,
        "url": ""
    }
}